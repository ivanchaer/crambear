li.card
  .labels
    button{bindAttr class=":card-label-button isArchived"}{action toggleArchived} archived
    partial "cards/labels"
  if isEditing
    partial "cards/edit"
  else
    #del-btn-container
      button.card-delete-btn{action delete}  x
    .front
      content.front
    .back
      content.back
